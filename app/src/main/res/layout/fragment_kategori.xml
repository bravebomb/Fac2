<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    android:background="#F5F5F5">

    <LinearLayout
        android:layout_marginTop="150dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:fontFamily="@font/bebas_regular"
            android:text="@string/Titel"
            android:textColor="@color/Fine_red"
            android:textSize="30dp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <androidx.appcompat.widget.AppCompatButton
            android:layout_marginRight="20dp"
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/foodsteak"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="kött och Fisk" />

        <androidx.appcompat.widget.AppCompatButton
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/foodapple"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Frukt och Grönt"/>
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <androidx.appcompat.widget.AppCompatButton
            android:layout_marginRight="20dp"
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/cow"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Mejeri" />

        <androidx.appcompat.widget.AppCompatButton
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/beer"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Dricka" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <androidx.appcompat.widget.AppCompatButton
            android:layout_marginRight="20dp"
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/candy"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Gott och Blandat" />

        <androidx.appcompat.widget.AppCompatButton
            android:background="@color/white"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/baguette"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Bröd och bullar" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <androidx.appcompat.widget.AppCompatButton
            android:layout_marginRight="20dp"
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/foodsteakoff"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Vegetariskt"
            android:background="@color/white"/>

        <androidx.appcompat.widget.AppCompatButton
            android:fontFamily="@font/bebas_regular"
            android:drawableTop="@drawable/cowoff"
            android:drawableTint="@color/Fine_red"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="20dp"
            android:text="Veganskt"
            android:background="@color/white"/>


    </LinearLayout>

</LinearLayout>